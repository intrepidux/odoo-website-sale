<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_website_config_settings" model="ir.ui.view">
            <field name="name">Website settings.inherited.website_sale_social_media</field>
            <field name="model">website.config.settings</field>
            <field name="inherit_id" ref="website.view_website_config_settings"/>
            <field name="arch" type="xml">
                <group string="Social Media" position="after">
                    <group string="Social Media Share">
                        <div class="oe_inline">
                            <field name="share_twitter"/>
                            <label for="share_twitter"/>
                            <field name="share_facebook"/>
                            <label for="share_facebook"/>
                            <field name="share_linkedin"/>
                            <label for="share_linkedin"/>
                            <field name="share_googleplus"/>
                            <label for="share_googleplus"/>
                        </div>
                    </group>
                </group>
            </field>
        </record>

    </data>
</openerp>
