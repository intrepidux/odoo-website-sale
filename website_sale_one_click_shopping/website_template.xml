<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <template id="product" inherit_id="website_sale.product">
            <xpath expr="//a[@id='add_to_cart']" position="attributes">
                <attribute name="href">javascript:void(0);</attribute>
            </xpath>
        </template>
        <template id="assets_frontend" inherit_id="website_sale.assets_frontend" name="Assests Frontend Website Sale Shopping Button">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/website_sale_one_click_shopping/static/src/js/main.js" />
            </xpath>
        </template>
    </data>
</openerp>
